{
    "Service": "S3",
    "Resources": [
        {
            "Resource": "Bucket",
            "Region": "eu-west-1 | eu-west-2",
            "Tags": {
                "Owner": "test@test.com",
                "Description": "This is a test bucket",
                "Team": "DevTeamTest"
            },
            "Web": {
                "Index": "index.html",
                "Error": "error.html",
                "Redirect": {
                    "Hostname": "host",
                    "protocol": "http/https"
                }
            },
            "Versioning": {
                "Status": "True"
            },
            "Replication": {
                "Role": "arn:aws:iam::{ACCOUNT}:role/iam-role-here",
                "Rules": [
                    {
                        "Id": "Replication Rule ID (Not Required)",
                        "Prefix": "~",
                        "Destination": {
                            "Account": "ACCOUNT_NUM",
                            "Bucket": "arn:aws:s3:::BUCKET-TO-REPLICATE-TO",
                            "Encryption": "EncryptionKey"
                        },
                        "SourceSelectionCriteria": {
                            "KMSEncryptionObjects": {
                                "Status": "True/False"
                            },
                            "Status": "True/False"
                        }
                    }
                ]
            },
            "BlockPublicAccess": {
                "BlockPublicACL": "True/False",
                "BlockPublicPolicy": "True/False",
                "IgnorePublicAcl": "True/False",
                "RestrictPublicBuckets": "True/False"
            },
            "Logging": {
                
            }
        }
    ]
}